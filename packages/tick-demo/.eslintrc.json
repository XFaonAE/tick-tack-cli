{
    "ignorePatterns": [".eslintrc.cjs"],
    "extends": ["airbnb", "airbnb-typescript"],
    "parserOptions": {
        "extraFileExtensions": [".cjs"],
        "project": "./tsconfig.json"
    },
    "rules": {
        "indent": ["error", 4],
        "@typescript-eslint/indent": ["error", 4],
        "max-len": ["error", { "code": 200 }],
        "import/no-cycle": [0]
    }
}
